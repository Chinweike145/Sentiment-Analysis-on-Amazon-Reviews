   "outputs": [],
   "source": [
    "def custom_encoder(df):\n",
    "    # Map each emotion to 1 (positive) or 0 (negative)\n",
    "    df['class'] = df['Reaction'].map({\n",
    "        'surprise': 1,\n",
    "        'love': 1,\n",
    "        'joy': 1,\n",
    "        'fear': 0,\n",
    "        'anger': 0,\n",
    "        'sadness': 0\n",
    "    })\n",
    "    return df"
   ]
  },
