    "# Initialize and train the SVM model\n",
    "svm_model = SVC(kernel='linear')  # Linear kernel works well for text classification\n",
    "svm_model.fit(X_train_tfidf, y_train)\n",
    "\n",
    "# Make predictions\n",
    "y_pred = svm_model.predict(X_test_tfidf)"
